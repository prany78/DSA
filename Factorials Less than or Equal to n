//optimised code
// User function Template for Java
class Solution {
    static ArrayList<Long> factorialNumbers(long n) {
        ArrayList<Long> arr=new ArrayList<>(); 
        long fact=1;
        long i=1;
        while(fact<=n)
        {
            arr.add(fact);
            i++;
            fact=fact*i;
        }
        return arr;
    }
}
